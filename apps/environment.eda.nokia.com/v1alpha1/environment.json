  "openapi": "3.0.1",
  "paths": {
    "/workflows/v1/environment.eda.nokia.com/v1alpha1/namespaces/{namespace}/cliplugins": {
      "post": {
        "description": "create a CliPlugin",
        "operationId": "createEnvironmentEdaNokiaComV1alpha1NamespaceCliplugins",
        "parameters": [
          {
            "description": "the namespace scope in which to create the CliPlugin",
            "in": "path",
            "name": "namespace",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/com.nokia.eda.environment.v1alpha1.CliPlugin"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WorkflowId"
                }
              }
            },
            "description": "The response for a successful POST workflows request."
          },
          "default": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "description": "Details of an error in response to an API REST request."
          }
        },
        "tags": [
          "workflows"
        ]
      }
    },
    "/workflows/v1/environment.eda.nokia.com/v1alpha1/namespaces/{namespace}/setupenvs": {
